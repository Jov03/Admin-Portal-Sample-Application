<% include ./partials/header%>
<% include ./partials/navbar%>
    
<form class="container CreateCompany" action="/Company/<%=company._id%>?_method=PUT" method="POST">
    <h2 class="text-center">Edit Company</h2>
    <div class="form-group">
        <label for="companyName">Company Name</label>
        <input class="form-control" name="Company[CompanyName]" id="companyName" type="text" value="<%=company.CompanyName%>"required>
    </div>
  
    <div class="form-group">
        <label for="address">Address</label>
        <input class="form-control" name="Company[Address]" id="address" type="text" value="<%=company.Address%>"required>
    </div>
    <div class="form-group">
        <label for="createdBy">Created By</label>
        <input class="form-control" name="Company[CreatedBy]" id="createdBy" type="tel" value="<%=company.CreatedBy%>" readonly>
    </div>
    


    <button class="btn btn-lg btn-primary btn-block">Add</button>
</form>




<% include ./partials/footer%>